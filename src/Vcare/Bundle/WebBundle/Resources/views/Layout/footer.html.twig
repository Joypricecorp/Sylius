{% spaceless %}
    {%- set footer_content -%}
        {{ render(url('toro_cms_partial_page', {slug: 'main-footer'})) }}
    {%- endset -%}
    <footer class="footer">
        <div class="footer__sitemap">
            <div class="container">
                {% if footer_content %}
                    {{ footer_content|raw }}
                {% else %}
                    {{ include('VcareWebBundle:Layout:footer-main-content.html.twig') }}
                {% endif %}
            </div>
        </div>
    </footer>
{% endspaceless %}
