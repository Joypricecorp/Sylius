{
    "name": "toro/media-bundle",
    "type": "symfony-bundle",
    "description": "Media management for Symfony2 projects.",
    "keywords": ["media"],
    "homepage": "http://github.com",
    "license": "ICS",
    "authors": [
        {
            "name": "Ishmael",
            "email": "nukboon@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",

        "symfony-cmf/media-bundle": "^1.3",
        "doctrine/phpcr-odm": "^1.3",
        "doctrine/phpcr-bundle": "^1.3",
        "jackalope/jackalope": "^1.1.4",
        "jackalope/jackalope-doctrine-dbal": "^1.1.2",
        "helios-ag/fm-elfinder-bundle": "~6"
    },
    "require-dev": {
        "symfony/form": "^2.8"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Toro\\Bundle\\MediaBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Toro\\Bundle\\MediaBundle\\spec\\": "spec/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.19-dev"
        }
    }
}
